{
  "name": "vaultcloud",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "worker:dev": "wrangler dev worker/src/index.ts",
    "worker:deploy": "wrangler deploy worker/src/index.ts",
    "worker:tail": "wrangler tail",
    "db:init": "wrangler d1 execute vaultcloud-db --file=./worker/src/db/schema.sql",
    "db:seed": "wrangler d1 execute vaultcloud-db --file=./worker/src/db/seed.sql"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251106.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wrangler": "^4.46.0"
  }
}
